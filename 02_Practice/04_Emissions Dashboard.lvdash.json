{
  "datasets": [
    {
      "name": "ec16ce42",
      "displayName": "Location Data",
      "queryLines": [
        "SELECT latitude, longitude, `GHG emissions mtons CO2e` AS Emissions\r\n",
        "FROM emissions.default.emissions_data"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "bb20bfbc",
      "displayName": "Emissions per Person",
      "queryLines": [
        "SELECT county_state_name, \r\n",
        "       population, \r\n",
        "       CAST(\r\n",
        "         REPLACE(`GHG emissions mtons CO2e`, ',', '') AS DOUBLE\r\n",
        "       ) / CAST(\r\n",
        "         REPLACE(population, ',', '') AS DOUBLE\r\n",
        "       ) AS Emissions_per_person\r\n",
        "FROM emissions.default.emissions_data\r\n",
        "ORDER BY emissions_per_person DESC"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "4b7440f4",
      "displayName": "Total Emissions per State",
      "queryLines": [
        "SELECT state_abbr, \r\n",
        "       SUM(CAST(\r\n",
        "         REPLACE(`GHG emissions mtons CO2e`, ',', '') AS DOUBLE\r\n",
        "       )) AS total_emissions\r\n",
        "FROM emissions_data\r\n",
        "GROUP BY state_abbr\r\n",
        "ORDER BY total_emissions DESC\r\n",
        "LIMIT 10"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "b60fb8f5",
      "displayName": "County Shaming",
      "queryLines": [
        "SELECT county_state_name, \r\n",
        "       population, \r\n",
        "       CAST(\r\n",
        "         REPLACE(`GHG emissions mtons CO2e`, ',', '') AS DOUBLE\r\n",
        "       ) AS total_emissions \r\n",
        "FROM emissions_data\r\n",
        "ORDER BY total_emissions DESC\r\n",
        "LIMIT 10"
      ],
      "catalog": "emissions",
      "schema": "default"
    }
  ],
